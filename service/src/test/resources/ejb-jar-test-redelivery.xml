<ejb-jar xmlns = "http://java.sun.com/xml/ns/javaee"
         version = "3.0"
         xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation = "http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd">
    <enterprise-beans>
        <session>
            <ejb-name>ReportServiceHelper</ejb-name>
            <ejb-local-ref>
                <ejb-ref-name>eu.europa.ec.fisheries.uvms.sales.service.bean.helper.ReportServiceHelper/rulesService
                </ejb-ref-name>
                <local>eu.europa.ec.fisheries.uvms.sales.service.RulesService</local>
                <ejb-link>SetTransactionRollbackRulesServiceAlternativeBean</ejb-link>
            </ejb-local-ref>
        </session>
        <session>
            <ejb-name>UnsavedMessageServiceBean</ejb-name>
            <ejb-local-ref>
                <ejb-ref-name>eu.europa.ec.fisheries.uvms.sales.service.bean.UnsavedMessageServiceBean/rulesService
                </ejb-ref-name>
                <local>eu.europa.ec.fisheries.uvms.sales.service.RulesService</local>
                <ejb-link>SetTransactionRollbackRulesServiceAlternativeBean</ejb-link>
            </ejb-local-ref>
        </session>
        <session>
            <ejb-name>ReportServiceBean</ejb-name>
            <ejb-local-ref>
                <ejb-ref-name>eu.europa.ec.fisheries.uvms.sales.service.bean.ReportServiceBean/rulesService
                </ejb-ref-name>
                <local>eu.europa.ec.fisheries.uvms.sales.service.RulesService</local>
                <ejb-link>SetTransactionRollbackRulesServiceAlternativeBean</ejb-link>
            </ejb-local-ref>
        </session>
  </enterprise-beans>
</ejb-jar>
